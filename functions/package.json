{
  "name": "functions",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "lint": "eslint -c eslint.config.ts --ext .js,.ts .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --import ./firebase-export --export-on-exit --only auth,functions,storage --inspect-functions=9229",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "rename-firebase-export": "mv firebase-export-* ./firebase-export",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "22"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios": "^1.12.2",
    "file-type": "^21.0.0",
    "firebase": "^12.6.0",
    "firebase-admin": "^13.5.0",
    "firebase-functions": "^7.0.0",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^3.1.0",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "private": true
}
